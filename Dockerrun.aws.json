{
	"AWSEBDockerrunVersion": 1,
	"containerDefinitions" : [
		{
			"name":"spring-cloud-config-server",
			"image":"sinch5/spring-cloud-config-server",
			"hostname": "spring-cloud-config-server",
			"essential": false,
			"memory": 128			
		},
		{
			"name": "nginx-proxy",
			"image": "nginx",
			"essential": true,
			"memory": 128,
			"portMappings": [
				{
					"hostPort": 5000,
					"containerPort": 5000
				}
			],
			"links": [
				"spring-cloud-config-server"
			]
		}
	]
}